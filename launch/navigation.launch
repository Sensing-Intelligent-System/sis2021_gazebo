<?xml version="1.0"?>
<launch>
  <arg name="debug" default="false"/>
  <arg name="scenario" default="navigation"/>
  <arg name="extra_gazebo_args" default=""/>
  <arg name="gui" default="true" />

  <param name="subt/gazebo_worlds_dir" value="$(find sis2021_gazebo)/worlds/Navigation" />

  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="world_name" value="$(find sis2021_gazebo)/worlds/Navigation/$(arg scenario).world" />
    <arg name="paused" value="false"/>
    <arg name="use_sim_time" value="true"/>
    <arg name="gui" value="$(arg gui)"/>
    <arg name="headless" value="false"/>
    <arg name="debug" value="$(arg debug)"/>
    <arg name="extra_gazebo_args" value="$(arg extra_gazebo_args) --verbose"/>
  </include>

</launch>
